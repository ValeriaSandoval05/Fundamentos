#include <iostream>
using namespace std;

int main()
{
    int i;
    cout<<"Salida del mientras"<<endl;
    i=0;
    while (i<10)
    {
        i++; 
        cout<<i<<endl;
    }
    cout<<"Salida del para"<<endl;
    for (int i=1;i<=10;i++)
        cout<<i<<endl;
cout<<"Salida del repetir"<<endl;
i=0;
do{
    i++;
    cout<<i<<endl;
} while (i<10);
return 0;
}
